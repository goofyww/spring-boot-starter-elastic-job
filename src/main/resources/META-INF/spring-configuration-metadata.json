{
  "properties": [
    {
      "name": "elastic-job.zookeeper.serverList",
      "type": "java.lang.String",
      "description": "Zookeeper server list, delimited by ,",
      "sourceType": "com.zcoox.boot.elasticjob.autoconfigure.ZookeeperProperties",
      "defaultValue": "localhost:2181"
    },
    {
      "name": "elastic-job.zookeeper.namespace",
      "type": "java.lang.String",
      "description": "Namespace for the jobs",
      "sourceType": "com.zcoox.boot.elasticjob.autoconfigure.ZookeeperProperties",
      "defaultValue": "elastic-job"
    },
    {
      "name": "elastic-job.zookeeper.baseSleepTimeMilliseconds",
      "type": "java.lang.Integer",
      "description": "等待重试的间隔时间的初始值. 单位毫秒",
      "sourceType": "com.zcoox.boot.elasticjob.autoconfigure.ZookeeperProperties",
      "defaultValue": 1000
    },
    {
      "name": "elastic-job.zookeeper.maxSleepTimeMilliseconds",
      "type": "java.lang.Integer",
      "description": "等待重试的间隔时间的最大值. 单位毫秒",
      "sourceType": "com.zcoox.boot.elasticjob.autoconfigure.ZookeeperProperties",
      "defaultValue": 3000
    },
    {
      "name": "elastic-job.zookeeper.maxRetries",
      "type": "java.lang.Integer",
      "description": "最大重试次数",
      "sourceType": "com.zcoox.boot.elasticjob.autoconfigure.ZookeeperProperties",
      "defaultValue": 3
    },
    {
      "name": "elastic-job.zookeeper.sessionTimeoutMilliseconds",
      "type": "java.lang.Integer",
      "description": "会话超时时间. 单位毫秒",
      "sourceType": "com.zcoox.boot.elasticjob.autoconfigure.ZookeeperProperties",
      "defaultValue": 0
    },
    {
      "name": "elastic-job.zookeeper.connectionTimeoutMilliseconds",
      "type": "java.lang.Integer",
      "description": "连接超时时间. 单位毫秒",
      "sourceType": "com.zcoox.boot.elasticjob.autoconfigure.ZookeeperProperties",
      "defaultValue": 0
    },
    {
      "name": "elastic-job.zookeeper.digest",
      "type": "java.lang.String",
      "description": "连接Zookeeper的权限令牌. 缺省为不需要权限验证",
      "sourceType": "com.zcoox.boot.elasticjob.autoconfigure.ZookeeperProperties",
      "defaultValue": null
    }
  ]
}